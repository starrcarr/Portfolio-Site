{
  "path": {
    "scss/*.scss": {
      "sass.enabled": true,
      "sass.options": {
          "outputDir": "../css/",
      "includePaths": [
                    "scss",
                ],
        
        "imagePath": null,
        "sourceComments": "map",
        "outputStyle": "compressed"
      }
    }
  }
}